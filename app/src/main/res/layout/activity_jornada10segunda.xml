<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:background="@drawable/cesped"
tools:context=".jornada10segunda">


<TextView
    android:id="@+id/tResultados"
    android:layout_width="299dp"
    android:layout_height="wrap_content"
    android:text="Resultados"
    android:textColor="#fff"
    android:textSize="40dp"
    android:textStyle="bold" />

<EditText
    android:id="@+id/editTextTextPersonName2"
    android:layout_width="347dp"
    android:layout_height="86dp"
    android:layout_below="@id/tResultados"
    android:layout_marginTop="-12dp"
    android:ems="10"
    android:inputType="textPersonName"
    android:text="Jornada 10"
    android:textSize="24dp"
    android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="342dp"
        android:layout_below="@id/editTextTextPersonName2"
        android:layout_marginTop="15dp"
        android:layout_marginBottom="20dp"
        android:src="@drawable/jornada10segunda" />

    <Button
    android:id="@+id/jornada9"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@id/imageView"
    android:layout_marginTop="13dp"
    android:text="jornada9" />

</RelativeLayout>